generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  user_id       Int    @id @default(autoincrement())
  user_name     String @db.VarChar(100)
  user_password String @db.VarChar(255)
  user_email    String @unique @db.VarChar(150)
}
